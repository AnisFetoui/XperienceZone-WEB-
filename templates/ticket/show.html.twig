{% extends 'base.html.twig' %}

{% block title %}Ticket{% endblock %}

{% block container %}
    <h2>Ticket</h2>

    <table class="table">
        <tbody>
           
            <tr>
                <th>NumTicket</th>
                <td>{{ ticket.numTicket }}</td>
            </tr>
            <tr>
                <th>Image</th>
                <td>{{ ticket.image }}</td>
            </tr>
            <tr>
                <th>Prix</th>
                <td>{{ ticket.prix }}</td>
            </tr>
            <tr>
                <th>Categorie</th>
                <td>{{ ticket.categorie }}</td>
            </tr>
        </tbody>
    </table>

    <a href="{{ path('app_ticket_index') }}">back to list</a>

    <a href="{{ path('app_ticket_edit', {'idTicket': ticket.idTicket}) }}">edit</a>

    {{ include('ticket/_delete_form.html.twig') }}
{% endblock %}
