{# inscription/_form.html.twig #}

<div class="inscription-form">
	{{ form_start(form, {'attr': {'novalidate': 'novalidate'}}) }}
	{{ form_label(form.nbrTickes) }}
	{{ form_widget(form.nbrTickes, {'attr': {'class': 'form-control'}}) }}
	{{ form_errors(form.nbrTickes) }}

	{{ form_label(form.fraitAbonnement) }}
	{{ form_widget(form.fraitAbonnement, {'attr': {'class': 'form-control'}}) }}
	{{ form_errors(form.fraitAbonnement) }}

	<button class="btn">{{ button_label|default('Save') }}</button>
	{{ form_end(form) }}
</div>
